{
  "name": "@mattlewis92/ngx-chips",
  "version": "2.0.0",
  "description": "Tag Input component for Angular",
  "scripts": {
    "prepublish": "npm run clean && npm run build",
    "build": "webpack && npm run ngc && npm run moveassets && npm run sass",
    "watch": "webpack -- --watch",
    "server": "webpack-dev-server --config webpack.demo.js --inline --colors --progress --port 3010 --content-base demo",
    "start": "npm run server",
    "test": "karma start karma.conf.js",
    "ngc": "ngc -p ./tsconfig.aot.json",
    "moveassets": "rsync -a --include='*.scss' --include='*/' --exclude='*' ./modules/ ./dist/modules/",
    "codecov": "codecov",
    "clean": "rm -rf ./dist",
    "sass": "node-sass ./modules/tag-input.module.scss dist/ngx-chips.css"
  },
